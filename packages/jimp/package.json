{
  "name": "jimp",
  "version": "1.0.0",
  "description": "An image processing library written entirely in JavaScript.",
  "files": [
    "browser",
    "dist",
    "es",
    "index.d.ts",
    "fonts",
    "types"
  ],
  "repository": "jimp-dev/jimp",
  "scripts": {
    "lint": "eslint .",
    "test-off": "vitest --watch=false",
    "build": "tshy",
    "dev": "tshy --watch",
    "clean": "rm -rf node_modules .tshy .tshy-build dist"
  },
  "keywords": [
    "image",
    "image processing",
    "image manipulation",
    "png",
    "jpg",
    "jpeg",
    "bmp",
    "resize",
    "scale",
    "crop"
  ],
  "author": "Andrew Lisowski <lisowski54@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@jimp/plugin-blit": "workspace:*",
    "@jimp/plugin-blur": "workspace:*",
    "@jimp/plugin-crop": "workspace:*",
    "@jimp/plugin-resize": "workspace:*",
    "@jimp/js-bmp": "workspace:*",
    "@jimp/js-gif": "workspace:*",
    "@jimp/js-jpeg": "workspace:*",
    "@jimp/js-png": "workspace:*",
    "@jimp/js-tiff": "workspace:*",
    "@jimp/core": "workspace:*"
  },
  "devDependencies": {
    "@jimp/config-eslint": "workspace:*",
    "@jimp/config-typescript": "workspace:*",
    "eslint": "^8.57.0",
    "tshy": "^1.12.0",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  },
  "tshy": {
    "exclude": [
      "**/*.test.ts"
    ],
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "type": "module"
}
